pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello'
                sleep 5
            }
        }
               stage('Buit') {
            steps {
                echo 'Buit'
                sleep 4
            }
        }
               stage('Test') {
            steps {
                echo 'Hello Test'
            }
        }
               stage('Deployment') {
            steps {
                echo 'Hello Deployment'
            }
        }
    }
}
